void setup()
{
  pinMode(1, OUTPUT);
  pinMode(2, OUTPUT);
  pinMode(3, OUTPUT);
}

void loop()
{
  digitalWrite(1, 127);
  delay(10000); 
  digitalWrite(2, 127);
  delay(10000);
  delay(2000);
  digitalWrite(1, 0);
  digitalWrite(2, 0);
  digitalWrite(3, 127);
  delay(5000); 
  digitalWrite(3, 0);
}
